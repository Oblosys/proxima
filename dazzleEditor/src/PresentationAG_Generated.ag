  | HoleSubgraph     lhs.presTree = presHole @lhs.focusD "Subgraph" (Node_HoleSubgraph @self @lhs.path) @lhs.path
  | ParseErrSubgraph lhs.presTree = presParseErr (Node_ParseErrSubgraph @self @lhs.path) @error

SEM Dirty
